  "description": "App for display wordpress post",
  "main": "index.js",
  "scripts": {
    "build": "npm run webpack -- --env.env=prod",
    "dev": "rm -rf dist/ && NODE_ENV=development node server.js",
    "build:bundlebuddy": "npm run webpack -- --env.env=prod --env.addons=bundlebuddy",
    "build:bundleanalyze": "npm run webpack -- --env.env=prod --env.addons=bundleanalyze",
    "format": "prettier --write --single-quote --print-width=120 --tab-width=2 \"src/**/*.{js,jsx}\" ",
    "clean": "rm -rf web/dist/",
    "test": "NODE_ENV=test node_modules/.bin/jest",
    "lint": "node_modules/.bin/eslint src/**/*.{js,jsx} --quiet",
    "flow": "node_modules/.bin/flow",
    "init": "npm i && flow-typed install",
    "webpack": "node_modules/.bin/webpack",
    "precommit": "lint-staged",
    "webpack-defaults": "webpack-defaults",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,json}": ["npm run format", "git add"]
